<rt-require dependency="../personInputRow/personInputRow" as="PersonInputRow"/>


<div class="app">
  <PersonInputRow key="{'person-' + person.id}" rt-repeat="person in this.props.nextEventPersons"
    id="{person.id}"
    name="{person.name}"
    status="{person.status}"
    updateStatus="{this.updateStatus}"
  />
</div>
